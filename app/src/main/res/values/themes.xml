<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Primary brand color. -->
    <style name="Theme.NavDrawer" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary" type="">@color/color_primary</item>
        <item name="colorPrimaryVariant" type="">@color/color_primary_variant</item>
        <item name="colorOnPrimary" type="">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary" type="">@color/color_secondary</item>
        <item name="colorSecondaryVariant">@color/color_secondary_variant</item>
        <item name="colorOnSecondary" type="">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l" type="">?attr/colorPrimaryVariant</item>

        <item name="android:buttonStyle" type="">@style/button</item>
        <item name="pageBackground">@style/page_background</item>
        <item name="textSubheader">@style/text_subheader</item>
        <item name="textLarge">@style/text_large</item>
        <item name="textRegular">@style/text_regular</item>
        <item name="whiteBackground">@style/white_background</item>
        <item name="button">@style/button</item>

    </style>
    <style name="Theme.NavDrawer.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NavDrawer.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NavDrawer.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>